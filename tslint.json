{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "trailing-comma": true,
    "space-before-function-paren": true,
    "prefer-const": true,
    "indent": [true, "spaces", 2],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
